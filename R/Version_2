  df19.sim<-df19.sim %>% mutate(col1=streak_run(state, k=1000))
  df19.sim<-df19.sim %>% filter(!(state=="4" & col1!=1))
  df19.sim<-df19.sim %>% filter(!(state=="5" & col1!=1))
  
