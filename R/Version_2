{
df1.sim<- as.data.frame(sim1.msm)
df1.sim<-df1.sim %>% gather(key = "id", value = "state")
df1.sim<-df1.sim %>% mutate(col1=streak_run(state, k=1000))
df1.sim<-df1.sim %>% filter(!(state=="4" & col1!=1))
df1.sim<-df1.sim %>% filter(!(state=="5" & col1!=1))

