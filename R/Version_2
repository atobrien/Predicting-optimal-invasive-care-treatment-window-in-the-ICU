                                                 state=="1"~u_nhic,
                                                 state=="3"~u_cmo,
                                                 state=="4"~u_discharge, 
                                                 state=="5"~u_died),
                              costs= case_when(state=="1"~c_nhic,
                                               state=="2"~c_hic,
                                               state=="3"~c_cmo,
                                               state=="4"~c_lastday,
                                               state=="5"~c_lastday)) 

# Get means of costs and utility 
