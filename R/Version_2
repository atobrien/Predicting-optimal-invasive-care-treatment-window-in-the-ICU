# SENSITIVITY ANALYSIS first order monte carlo simulations cmo 14 ---------

# 1 way sensitivity analysis altering hit to cmo probabilities

B <- 100
N <- 30

{
sim1.msm <- replicate(B, {
  X <- rmarkovchain(n = N, 
                    object = mo1.msm, 
                    t0 = "2",
                    include.t0 = TRUE,
                    parallel = TRUE,
                    num.cores=3)})

