  mo9.msm<-new("markovchain",transitionMatrix=(sens_matrix.cmo76_9), name="MarkovChain Q1")
  mo10.msm<-new("markovchain",transitionMatrix=(sens_matrix.cmo76_10), name="MarkovChain Q1")
