                    object = mo13.msm, 
                    t0 = "2",
                    include.t0 = TRUE,
                    parallel = TRUE,
                    num.cores=3)})

sim14.msm <- replicate(B, {
  X <- rmarkovchain(n = N, 
                    object = mo14.msm, 
                    t0 = "2",
                    include.t0 = TRUE,
                    parallel = TRUE,
                    num.cores=3)})

sim15.msm <- replicate(B, {
  X <- rmarkovchain(n = N, 
                    object = mo15.msm, 
                    t0 = "2",
                    include.t0 = TRUE,
                    parallel = TRUE,
                    num.cores=3)})

